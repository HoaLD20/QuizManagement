<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">

    <servlet>
        <servlet-name>Index</servlet-name>
        <servlet-class>Controller.Index</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>Controller.Login</servlet-class>
    </servlet>
    
        <servlet>
        <servlet-name>MakeQuiz</servlet-name>
        <servlet-class>Controller.MakeQuiz</servlet-class>
    </servlet>
    
    
    <servlet>
        <servlet-name>ManageQuiz</servlet-name>
        <servlet-class>Controller.ManageQuiz</servlet-class>
    </servlet>
    
        <servlet>
        <servlet-name>TakeQuiz</servlet-name>
        <servlet-class>Controller.TakeQuiz</servlet-class>
    </servlet>
    
    
    
    <servlet>
        <servlet-name>Logout</servlet-name>
        <servlet-class>Controller.Logout</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>Register</servlet-name>
        <servlet-class>Controller.Register</servlet-class>
    </servlet>
    
    
        <servlet-mapping>
        <servlet-name>Index</servlet-name>
        <url-pattern>/Index</url-pattern>
    </servlet-mapping>
    
    
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    
        <servlet-mapping>
        <servlet-name>MakeQuiz</servlet-name>
        <url-pattern>/MakeQuiz</url-pattern>
    </servlet-mapping>
    
        <servlet-mapping>
        <servlet-name>ManageQuiz</servlet-name>
        <url-pattern>/ManageQuiz</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>TakeQuiz</servlet-name>
        <url-pattern>/TakeQuiz</url-pattern>
    </servlet-mapping>
    
    
    
    <servlet-mapping>
        <servlet-name>Logout</servlet-name>
        <url-pattern>/Logout</url-pattern>
    </servlet-mapping>


    <servlet-mapping>
        <servlet-name>Register</servlet-name>
        <url-pattern>/Register</url-pattern>
    </servlet-mapping>

    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    
    
    
    <welcome-file-list>
        <welcome-file>Index</welcome-file>
    </welcome-file-list>
</web-app>
